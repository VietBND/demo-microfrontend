{
  "projectType": "library",
  "sourceRoot": "libs/flight/feature-flight/src",
  "prefix": "demo-microfrontend",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/flight/feature-flight"],
      "options": {
        "jestConfig": "libs/flight/feature-flight/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/flight/feature-flight/**/*.ts",
          "libs/flight/feature-flight/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
